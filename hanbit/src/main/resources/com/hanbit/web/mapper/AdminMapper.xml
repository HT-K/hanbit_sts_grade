<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hanbit.web.mapper.AdminMapper">
  <select id="login" resultType="com.hanbit.web.admin.AdminDTO" parameterType="com.hanbit.web.admin.AdminDTO">
  	SELECT * FROM Admin 
  	where id = #{id} and password = #{password}
  </select>
  	
</mapper>